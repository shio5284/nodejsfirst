<%- include('../_partial/_header.ejs') %>
<h3>Sign In</h3>



<form action ="/signup" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1" class="sr-only mt-md-3">User name</label>
        <input type="input" class="form-control" name="username"  placeholder="User Name">
       
      </div>
    <div class="form-group">
      <label for="exampleInputEmail1" class="sr-only mt-md-3">Email address</label>
      <input type="email" class="form-control" name="email" placeholder="Enter email">
     
    </div>
    <div class="form-group">
      <label for="Password1" class="sr-only mt-md-3">Password</label>
      <input type="password" class="form-control" name="password" placeholder="Password">
    </div>
    <div class="form-group">
        <label for="password" class="sr-only mt-md-3">Re-Password</label>
        <input type="password" class="form-control" name="password1" placeholder="Re-Password">
      </div>

    <div class="form-check">
      <input type="checkbox" class="form-check-input class="sr-only mt-md-3" id="exam">
      <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>

  <%- include('../_partial/_footer.ejs') %>